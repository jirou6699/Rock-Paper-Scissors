{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:styles": "sass ./scss/style.scss ../assets/stylesheets/style.css && postcss --use autoprefixer --no-map ../assets/stylesheets/style.css -r",
    "watch:styles": "onchange \"scss/**/*.scss\" -i -- npm run dev:styles",
    "build:styles": "sass --style=compressed ./scss/style.scss ../assets/stylesheets/style.css && postcss --use autoprefixer --no-map ../assets/stylesheets/style.css -r",
    "start": "npm run watch:styles",
    "build": "npm run build:styles"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "onchange": "^7.1.0",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "sass": "^1.69.5"
  }
}
